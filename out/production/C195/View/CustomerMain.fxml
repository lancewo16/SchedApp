<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="562.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerMain">
    <children>
        <Label alignment="CENTER" layoutX="342.0" layoutY="14.0" prefHeight="57.0" prefWidth="316.0" text="CUSTOMERS " textAlignment="CENTER" underline="true">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <TableView fx:id="customerTableView" layoutX="34.0" layoutY="83.0" prefHeight="400.0" prefWidth="1100.0">
            <columns>
                <TableColumn fx:id="customerIdTableColumn" prefWidth="110.0" text="ID" />
                <TableColumn fx:id="nameTableColumn" prefWidth="110.0" text="Name" />
            <TableColumn fx:id="addressTableColumn" prefWidth="110.0" text="Address" />
            <TableColumn fx:id="postalTableColumn" prefWidth="110.0" text="Postal Code" />
            <TableColumn fx:id="phoneTableColumn" prefWidth="110.0" text="Phone" />
            <TableColumn fx:id="createDateTableColumn" prefWidth="110.0" text="Date Created" />
            <TableColumn fx:id="createdByTableColumn" prefWidth="110.0" text="Created By" />
            <TableColumn fx:id="lastUpdateTableColumn" prefWidth="110.0" text="Last Update" />
            <TableColumn fx:id="lastUpdatedByTableColumn" prefWidth="110.0" text="Last Updated By" />
            <TableColumn fx:id="divisionIDTableColumn" prefWidth="110.0" text="Division ID" />
            </columns>
        </TableView>
        <Button fx:id="addCustomerButton" layoutX="882.0" layoutY="494.0" mnemonicParsing="false" onAction="#onActionSceneAddCustomer" prefHeight="27.0" prefWidth="73.0" text="ADD" />
        <Button fx:id="deleteCustomerButton" layoutX="1063.0" layoutY="494.0" mnemonicParsing="false" onAction="#onActionSceneDeleteCustomer" prefHeight="27.0" prefWidth="73.0" text="DELETE" />
        <Button fx:id="editCustomerButton" layoutX="974.0" layoutY="494.0" mnemonicParsing="false" onAction="#onActionSceneEditCustomer" prefHeight="27.0" prefWidth="73.0" text="EDIT" />
        <Button fx:id="mainButton" layoutX="884.0" layoutY="527.0" mnemonicParsing="false" onAction="#onActionSceneMain" prefHeight="27.0" prefWidth="73.0" text="MAIN" />
        <Button fx:id="exitButton" layoutX="1065.0" layoutY="527.0" mnemonicParsing="false" onAction="#onActionExitApp" prefHeight="27.0" prefWidth="73.0" text="EXIT" />
      <Button fx:id="logoutButton" layoutX="976.0" layoutY="527.0" mnemonicParsing="false" onAction="#onActionSceneLogout" prefHeight="27.0" prefWidth="74.0" text="LOGOUT" />
    </children>
</AnchorPane>
